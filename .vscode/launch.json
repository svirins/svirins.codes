{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Next.js",
      "type": "node-terminal",
      "request": "launch",
      "command": "bun dev",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ]
}
